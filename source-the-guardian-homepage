function storeCss(c){Object.keys(localStorage).forEach(function(key){if(key.match(/^gu.css./g)){localStorage.removeItem(key);}});try{localStorage.setItem('gu.css.9c7405e9339d6841d1d9572998fdbff1',c);}catch(e){}}
