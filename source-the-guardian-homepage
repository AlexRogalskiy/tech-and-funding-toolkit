function loadCssWithAjax(){var xhr=new XMLHttpRequest();xhr.open('GET','http://assets.guim.co.uk/stylesheets/9c7405e9339d6841d1d9572998fdbff1/global.css?page=ajax',true);xhr.onreadystatechange=function(){if(xhr.readyState===4){if(xhr.status===200){inlineCss(xhr.responseText);storeCss(xhr.responseText);}else{loadCssWithLink();};}}
xhr.timeout=5000;xhr.send();}
