				window['win8AdCall'] = function() { /* do nothing */ };
			}
			var ad_tag = 'http://ad.doubleclick.net/N6556/adj/' + BF_DFP_SITE + BF_DFP_ZONE + ';wid=2;page=1;sz=5x5;pos=story2;tile=2;' + BF_DFP_KVS + ';' + BF_TAG + win8AdCall() +  '';
			if (typeof AD_DESIGN != 'undefined') { ad_tag += ';abtest=' + AD_DESIGN + ';' }

			//Leinenkugel targeted ads
			if(window.location.search.match('leinenkugel') && BF_STATIC.tt_page === "Buzz"){
				if(window.location.search.match('leinenkugel=calendar_sp')) ad_tag += 'leinenkugel=calendar_sp;'
				else if(window.location.search.match('leinenkugel=calendar_edit')) ad_tag += 'leinenkugel=calendar_edit;'
				else if(window.location.search.match('leinenkugel=carousel_sp')) ad_tag += 'leinenkugel=carousel_sp;'
				else if (window.location.search.match('leinenkugel=carousel_edit')) ad_tag += 'leinenkugel=carousel_edit;'
			}

			//Loreal MUC channel
			if(window.location.search.match('loreal_feed=1') && BF_STATIC.tt_page === "Buzz"){
				ad_tag += 'loreal_feed=1;'
			}


			ad_tag += 'poe=' + BF_REFERER + ';';
			ad_tag += 'ord=' + BF_DFP_ORD + '?';
		
		

		
		if(ad_tag) {
			if (!('wid=2;page=1;sz=5x5;pos=story2;tile=2').match(/pos=300x250/) && null !== window.location.search.match("skipdfp")) {
				window.addEventListener('load', function(){
					var query = (typeof String.prototype.toQueryParams != 'undefined') ? window.location.search.toQueryParams() : $.deparam.querystring();
					var ad_user = (query['skipdfp']) ? query['skipdfp'] : 'boost';
					var extra_params = (/bigstory/.test('wid=2;page=1;sz=5x5;pos=story2;tile=2')) ? '&flex_medium=bigstory' : '';
					local_ad_call( 2, ad_user, extra_params );
				})
			} else {
				if(!BF_STATIC.bf_test_mode || document.cookie.match('sel2_ad') !== null ) {
					document.write('<'+'scr'+'ipt type="text/javascr'+'ipt" src="'+ad_tag+'"><\/scr'+'ipt'+'>');
				} else {
					console.info('disable DFP (dfp_tags.tt)');
				}
			}
		}
		

	    if (null !== window.location.search.match("adtest") && console !== undefined && typeof ad_tag != "undefined") console.log(ad_tag);
	  
	</script>
					
	

		



