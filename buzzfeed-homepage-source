				if(window.location.search.match('leinenkugel=calendar_sp')) ad_tag += 'leinenkugel=calendar_sp;'
				else if(window.location.search.match('leinenkugel=calendar_edit')) ad_tag += 'leinenkugel=calendar_edit;'
