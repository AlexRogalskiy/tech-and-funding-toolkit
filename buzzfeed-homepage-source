<script type="text/javascript">
var flex_data = [];
var revs = 100;
var flex_ord = Math.round(Math.random()*revs);
var buckets = $R(0, revs - 1).map( function(i) { return []; } );

flex_data.each( function(buzz) {
	var running_chance = 0;
	buzz.promos.each(function(p){

		// Here we figure out for each value between 1 and $revs, which promo to show
	  // Might get some overlap/gaps in here, but it should show the default in that case
		var start = Math.floor( revs * running_chance );
		var end   = start + Math.ceil( revs * p.probability ) - 1;
		running_chance += p.probability;

		$R(start, end).each(function(i){
			buckets[i].push(p);
		});

	});
});

var bucket = buckets[flex_ord];
var none = [];
var block = [];
var BF_SELECTED_PROMOS = {};
bucket.each(function(promo){
	// return if
	if (BF_SELECTED_PROMOS[promo.campaign_id]) return false;
	none.push( "#post-" + promo.campaign_id + " .flex-default" );
	block.push( "#post-" + promo.campaign_id + " .flex-" + promo.promo_id );
	BF_SELECTED_PROMOS[promo.campaign_id] = promo;
});

var css = [ '<style>', none.join(', ') + ' { display: none !important; }', block.join(', ') + ' { display: block !important; }', '</style>' ];

document.write( css.join("\n") );

</script>

