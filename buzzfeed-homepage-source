	  								<li class=" bf_dom bf_cts-0-3 bf_cts" rel:bf_cts="0:3:3440067:buzz"  rel:bf_bucket="track" track="{&quot;c&quot;:&quot;7EC4NA7&quot;,&quot;u&quot;:&quot;76RF8E7&quot;,&quot;buzz&quot;:&quot;taylor-swift-bad-blood-katy-perry-s6vj&quot;,&quot;user&quot;:&quot;kelleylocke&quot;,&quot;types&quot;:[100],&quot;queries&quot;:[]}" id="promoted-3440067">
						<div class="ie-hot-unit"></div>

