<div class="weather"
    style="display: none;"
    data-track-module="nav-weather^weather" data-track-pos="static"></div>
<script type="text/javascript">
    (function () {
    var now = moment();
    var offset = -1 * new Date().getTimezoneOffset();
    var m = moment().subtract(offset, "minutes");
    if (m.minutes() != 0) { m = m.add(1, "hours") };
    var tod = function (m) {
      var mod3 = m.hours() % 3;
      return (!mod3 ? m : m.add(3 - mod3, "hours")).add(offset, "minutes").format("hA");
    };
 
    $.get( "/weather.html", {
      old: "true",
      latitude: PageCriteria.latitude,
      longitude: PageCriteria.longitude,
      
      weatherDate: '2014091005'}, function( data ) {
        $( ".page-header .weather" ).append( "<strong>" + now.format("dddd, MMM Do YYYY") + "</strong>" + data );
        
        var anc = $(".page-header .weather a");
        if (PageCriteria.geo != "GB" && anc) 
          anc.attr("href", anc.attr("href") + "?mapId=17&numDays=0&mapType=Weather&temp=");
          var spans = $(".page-header .weather span");
          spans[0].innerHTML = tod(m.clone()) + "&nbsp;";
          spans[2].innerHTML = tod(m.clone().add(3, "hours")) + "&nbsp;";
          $(".page-header .weather").show();
      });
    })();

</script>


